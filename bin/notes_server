#!/usr/bin/env ruby
$LOAD_PATH.unshift File.realdirpath('../../lib', __FILE__)
require_relative '../lib/notes/web'
require_relative '../lib/notes/app'
require 'net/http'

class Go
  include Appmod
  port = 9292
  server = Notes::Web.new(App, Port: port, Host: 'localhost')
  server.start
end

Go.new
